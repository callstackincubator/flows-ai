{
  "name": "ai-flows",
  "version": "0.1.2",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.19",
    "ai": "^4.0.36",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@release-it-plugins/workspaces": "^4.2.0",
    "@release-it/conventional-changelog": "^9.0.3",
    "@clack/prompts": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "bun-types": "^1.1.33",
    "eslint": "^8.21.0",
    "release-it": "^17.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "^3.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "prepare": "bun run build",
    "clean": "rm -rf dist",
    "build": "tsup-node"
  },
  "author": "Mike Grabowski <grabbou@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callstackincubator/workflows-ai.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai",
    "ai agents",
    "ai tools",
    "ai workflows",
    "ai sdk"
  ],
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "engines": {
    "node": ">=22"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "requireCleanWorkingDir": false,
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    }
  }
}
